<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
   	<application>
        <resource-bundle>
            <base-name>messages</base-name>
            <var>msgs</var>
        </resource-bundle>
        <message-bundle>JSF-override</message-bundle>
	</application>
	
	<managed-bean>
		<managed-bean-name>userInfoBackingBean</managed-bean-name>
		<managed-bean-class>com.haochi.facade.backingbean.userinfo.UserInfoBackingBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
	
	<managed-bean>
		<managed-bean-name>userRegisterBackingBean</managed-bean-name>
		<managed-bean-class>com.haochi.facade.backingbean.userinfo.UserRegisterBackingBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>	
	
	<managed-bean>
		<managed-bean-name>bookServiceBackingBean</managed-bean-name>
		<managed-bean-class>com.haochi.facade.backingbean.bookservice.BookServiceBackingBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>	
	
	<managed-bean>
		<managed-bean-name>selectionServiceBackingBean</managed-bean-name>
		<managed-bean-class>com.haochi.facade.backingbean.selection.SelectFunctionBackingBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>orderBackingBean</managed-bean-name>
		<managed-bean-class>com.haochi.facade.backingbean.bookservice.OrderBackingBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

</faces-config>
